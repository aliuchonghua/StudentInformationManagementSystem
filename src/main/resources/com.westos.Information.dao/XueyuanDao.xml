<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.westos.Information.dao.XueyuanDao">
    <!--通过xueid查找学院-->
    <select id="findxueyuan" resultType="com.westos.Information.bean.Xueyuan">
        SELECT * FROM xueyuan WHERE xueid=#{xueid}
    </select>
</mapper>
